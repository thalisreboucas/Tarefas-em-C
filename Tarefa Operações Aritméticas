int main(void) { 
int a=1; 
int b=2;
int f=-1;
int x=(sqrt((2*b)-(4*a))+(2*(pow(f,-3))))/3-(2*a);
printf(" %d ", x); 
}

\\q)
#include <stdio.h>
#include <stdlib.h>
#include <math.h>

int main(void) {
	float x = 2;
	float H = 1;
	float y = 2*H-pow(((45/3*x)-4*H*(3-H)),2*H);
	printf("%f\n", y);
	return 0;
}


\\r)
#include <stdio.h>
#include <math.h>
int main(void) {
int a = 1 ;
int b = 2 ;
int c = 3 ;
int d=  5 ;
int e = 1 ;
int f = 3 ;
float g = 2.5 ;
int h = 12 ;
int i = 5 ;
float j = 0.5 ;
float x = (a - b) +(e+f) /(c -d) + (g+h)* (i/j);
printf("%f ",x );
}
#include <stdio.h>
int main(void) {
 float a = 1;
 float b = 2;
 float m = (a/b);
   printf("resultado da divisão real %f/%f = %f ",a,b,m);
	 printf("resultado da divisão inteiro  %d ",m);

 int x = 1;
 int y = 2;
 int d = (a/b);
  printf("resultado da divisão inteiro %d/%d = %d ",x,y,d);
 printf("resultado da divisão inteiro  %f ",d);

 int e = 1;
 float r = 2;
 printf("resultado da divisão inteiro %d",e/r);
 printf("resultado da divisão inteiro  %f ",e/r);

 float u = 1;
 int i = 2;
 printf("resultado da divisão inteiro %d ",u/i);
 printf("resultado da divisão inteiro  %f ",u/i);
}


// No comando "int" o computador entende que é um número inteiro e que não tem casas decimais , 
já no float haverá casa decimais ,mas o importante é definir no printf qual saída vou querer , mas vai gerar um erro.




