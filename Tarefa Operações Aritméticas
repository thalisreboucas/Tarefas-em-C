Thalis Rebouças

a)
#include <stdio.h>
int main(void) {
 float a = 1;
 float b = 2;
 float m = (a/b);
  printf("resultado da divisão real %f/%f = %f ",a,b,m);
}

\\b)
#include <stdio.h>
int main(void) {
 int a = 1;
 int b = 2;
 int m = (a/b);
  printf("resultado da divisão inteiro %d/%d = %d ",a,b,m);
}

\\c)
