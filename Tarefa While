Thalis Rebouças

a)
#include <stdio.h>

int main(void) {
	int x,y;
		printf("digite um valor para x\n");
		scanf("%d",&x);
	
		printf("digite um valor para y\n");
		scanf("%d",&y);

		if (y == x)
		printf("eles tem mesmo valor");
		
		else 
			if (x>y)
			printf("x é maior que y");
			else 
			printf("y é maior que x");

  return 0;
}
